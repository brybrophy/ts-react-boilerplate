{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",
        "lib": ["es6", "dom"],
        "noImplicitAny": false,
        "sourceMap": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "moduleResolution": "node",
        "noUnusedLocals": true,
        "rootDir": "src/",
        "outDir": "dist/",
        "jsx": "react"
    },
    "compileOnSave": false,
    "exclude": ["node_modules"]
}
